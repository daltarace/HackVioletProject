<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondAboutActivity">

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="242dp"
        android:layout_height="136dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.229"
        tools:text="This app is great for helping track the expiration dates for all your food, cosmetic products, and more! " />

    <EditText
        android:id="@+id/editTextTextMultiLine2"
        android:layout_width="225dp"
        android:layout_height="187dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="346dp"
        tools:text="It can additionally send you a notification anytime one of your items expires and reminds you to purchase a new one." />
</androidx.constraintlayout.widget.ConstraintLayout>